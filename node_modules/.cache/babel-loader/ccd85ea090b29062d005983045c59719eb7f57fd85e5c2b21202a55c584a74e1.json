{"ast":null,"code":"import { columns } from \"../../data/Kanban/defaultdemo\";\nimport { VueDraggableNext } from 'vue-draggable-next';\nexport default {\n  components: {\n    draggable: VueDraggableNext\n  },\n  data() {\n    return {\n      columns: columns\n    };\n  }\n};","map":{"version":3,"names":["columns","VueDraggableNext","components","draggable","data"],"sources":["D:\\Wrenix\\Spring-Sfa\\Cuba\\template\\template\\src\\pages\\kanban\\CustomBoardCard.vue"],"sourcesContent":["<template>\n<div class=\"col-12\">\n    <div class=\"card\">\n        <div class=\"card-header \">\n            <h3>Custom Board </h3>\n            <p class=\"mb-0\">| colors, gutter, click on board&apos;s item and restricting which boards to drag items to </p>\n        </div>\n        <div class=\"card-body\">\n            <div id=\"demo2\">\n                <div class=\"kanban-container\" style=\"width: 1290px;\">\n                    <draggable>\n                        <div data-id=\"_todo\" data-order=\"1\" class=\"kanban-board\" style=\"width: 400px; margin-left: 15px; margin-right: 15px;position: relative;float: left;\" v-for=\"index in columns\" :key=\"index.title\">\n                            <header class=\"kanban-board-header bg-info\">\n                                <div class=\"kanban-title-board\">{{index.title}} (Item only in Working)</div>\n                            </header>\n                            <draggable :list=\"index.tasks\" :animation=\"200\" ghost-class=\"ghost-card\" group=\"tasks\" style=\"min-height: 100px\">\n                                <div class=\"kanban-item\" v-for=\"(task) in index.tasks\" :key=\"task.id\">\n                                    <a class=\"kanban-box\" href=\"javascript:void(0)\" data-bs-original-title=\"\" title=\"\">\n                                        <span class=\"date\">{{task.date}}</span>\n                                        <span class=\"badge f-right\" :class=\"task.badgetype\">{{task.badge}}</span>\n                                        <h6>{{task.title}}</h6>\n                                        <div class=\"media\">\n                                            <img class=\"img-20 me-1 rounded-circle\" src=\"@/assets/images/user/3.jpg\" alt=\"\" data-original-title=\"\" title=\"\">\n                                            <div class=\"media-body\">\n                                                <p>{{task.location}}</p>\n                                            </div>\n                                        </div>\n                                        <div class=\"d-flex mt-3\">\n                                            <ul class=\"list\">\n                                                <li><i class=\"fa fa-comments-o\"></i>2</li>\n                                                <li><i class=\"fa fa-paperclip\"></i>2</li>\n                                                <li><i class=\"fa fa-eye\"></i></li>\n                                            </ul>\n                                            <div class=\"customers\">\n                                                <ul>\n                                                    <li class=\"d-inline-block me-3\">\n                                                        <p class=\"f-12\">+5</p>\n                                                    </li>\n                                                    <li class=\"d-inline-block\"><img class=\"img-20 rounded-circle\" src=\"@/assets/images/user/3.jpg\" alt=\"\" data-original-title=\"\" title=\"\"></li>\n                                                    <li class=\"d-inline-block\"><img class=\"img-20 rounded-circle\" src=\"@/assets/images/user/1.jpg\" alt=\"\" data-original-title=\"\" title=\"\"></li>\n                                                    <li class=\"d-inline-block\"><img class=\"img-20 rounded-circle\" src=\"@/assets/images/user/5.jpg\" alt=\"\" data-original-title=\"\" title=\"\"></li>\n                                                </ul>\n                                            </div>\n                                        </div>\n                                    </a>\n                                </div>\n                            </draggable>\n                            <footer></footer>\n                        </div>\n                    </draggable>\n                </div>\n            </div>\n        </div>\n    </div>\n</div>\n</template>\n\n<script>\nimport {\n    columns\n} from \"../../data/Kanban/defaultdemo\"\nimport {\n    VueDraggableNext\n} from 'vue-draggable-next'\nexport default {\n    components: {\n        draggable: VueDraggableNext,\n    },\n    data() {\n        return {\n            columns: columns\n        }\n    },\n}\n</script>\n\n<style scoped>\n.kanban-container {\n    /* position: relative; */\n    /* box-sizing: border-box; */\n    width: auto;\n}\n\n.kanban-container * {\n    box-sizing: border-box;\n}\n\n.kanban-container:after {\n    clear: both;\n    display: block;\n    content: \"\";\n}\n\n.kanban-board {\n    position: relative;\n    float: left;\n    transition: all 0.3s cubic-bezier(0.23, 1, 0.32, 1);\n}\n\n.kanban-board.disabled-board {\n    opacity: 0.3;\n}\n\n.kanban-board header {\n    font-size: 16px;\n    padding: 20px 30px;\n}\n\n.kanban-board header .kanban-title-board {\n    font-weight: 500;\n    margin: 0;\n    padding: 0;\n    display: inline;\n}\n\n.kanban-board header .kanban-title-button {\n    float: right;\n}\n\n.kanban-board .kanban-drag {\n    min-height: 200px;\n    padding: 20px;\n}\n\n.kanban-board:after {\n    clear: both;\n    display: block;\n    content: \"\";\n}\n\n.kanban-board.is-moving.gu-mirror {\n    transform: rotate(3deg);\n}\n\n.kanban-board.is-moving.gu-mirror .kanban-drag {\n    overflow: hidden;\n    padding-right: 50px;\n}\n\n.kanban-item {\n    background: #f8f8f8;\n    padding: 15px;\n    transition: all 0.3s cubic-bezier(0.23, 1, 0.32, 1);\n    margin-bottom: 0;\n}\n\n.kanban-item:hover {\n    cursor: move;\n}\n\n.kanban-item:last-child {\n    margin: 0;\n}\n\n.kanban-item.is-moving.gu-mirror {\n    transform: rotate(3deg);\n    height: auto !important;\n}\n\n#myKanban {\n    overflow-x: auto;\n    padding: 20px 0;\n}\n</style>\n"],"mappings":"AA0DA,SACIA,OAAM,QACH,+BAA8B;AACrC,SACIC,gBAAe,QACZ,oBAAmB;AAC1B,eAAe;EACXC,UAAU,EAAE;IACRC,SAAS,EAAEF;EACf,CAAC;EACDG,IAAIA,CAAA,EAAG;IACH,OAAO;MACHJ,OAAO,EAAEA;IACb;EACJ;AACJ","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}
{"ast":null,"code":"import { mapState } from \"vuex\";\nimport getImage from \"@/mixins/getImage\";\nexport default {\n  mixins: [getImage],\n  data() {\n    return {\n      counter: 1\n    };\n  },\n  computed: {\n    ...mapState({\n      cart: state => state.product.cart,\n      getAmount() {\n        return this.totalAmount = this.$store.getters[\"product/getTotalAmount\"];\n      }\n    })\n  },\n  methods: {\n    removeProduct: function (product) {\n      this.$store.dispatch(\"product/removeProduct\", product);\n    },\n    increment(product, qty = 1) {\n      this.$store.dispatch(\"product/updateCartQuantity\", {\n        product: product,\n        qty: qty\n      });\n    },\n    decrement(product, qty = -1) {\n      this.$store.dispatch(\"product/updateCartQuantity\", {\n        product: product,\n        qty: qty\n      });\n    }\n  }\n};","map":{"version":3,"names":["mapState","getImage","mixins","data","counter","computed","cart","state","product","getAmount","totalAmount","$store","getters","methods","removeProduct","dispatch","increment","qty","decrement"],"sources":["D:\\Wrenix\\Spring-Sfa\\Cuba\\template\\template\\src\\pages\\ecommerce\\cart\\productCart.vue"],"sourcesContent":["<template>\n    <table class=\"table table-bordered\">\n        <thead>\n            <tr>\n                <th>Prdouct</th>\n                <th>Prdouct Name</th>\n                <th>Price</th>\n                <th>Quantity</th>\n                <th>Action</th>\n                <th>Total</th>\n            </tr>\n        </thead>\n        <tbody>\n            <tr v-for=\"(item, index) in cart\" :key=\"index\">\n                <td><img class=\"img-fluid img-40\" :src='getImageUrl(item.img)' alt=\"#\"></td>\n                <td>\n                    <div class=\"product-name\">\n                        <router-link :to=\"'/ecommerce/product/page/' + item.id\">\n                            <h6>{{ item.name }}</h6>\n                        </router-link>\n                    </div>\n                </td>\n                <td>${{ item.price }}</td>\n                <td>\n                    <fieldset class=\"qty-box\">\n                        <div class=\"input-group bootstrap-touchspin\">\n                            <button class=\"btn btn-primary btn-square bootstrap-touchspin-down\" v-on:click=\"decrement(item)\"\n                                type=\"button\">\n                                <i class=\"fa fa-minus\"></i></button>\n                            <input class=\"touchspin text-center form-control\" v-model=\"item.quantity\" name=\"item.quantity\"\n                                type=\"text\">\n                            <button class=\"btn btn-primary btn-square bootstrap-touchspin-up\" v-on:click=\"increment(item)\"\n                                type=\"button\">\n                                <i class=\"fa fa-plus\"></i></button>\n                        </div>\n                    </fieldset>\n                </td>\n                <td><a href=\"javascript:void(0)\" @click='removeProduct(item)'>\n                        <vue-feather type=\"x-circle\"></vue-feather>\n                    </a></td>\n                <td>${{ item.price * item.quantity }}</td>\n            </tr>\n            <tr>\n                <td colspan=\"4\">\n                    <div class=\"input-group\">\n                        <input class=\"form-control me-2\" type=\"text\" placeholder=\"Enter coupan code\"><a\n                            class=\"btn btn-primary\" href=\"#\">Apply</a>\n                    </div>\n                </td>\n                <td class=\"total-amount\">\n                    <h6 class=\"m-0 text-end\"><span class=\"f-w-600\">Total Price :</span></h6>\n                </td>\n                <td><span>${{ getAmount }} </span></td>\n            </tr>\n            <tr>\n                <td class=\"text-end\" colspan=\"5\">\n                    <router-link :to=\"'/ecommerce/product'\" class=\"btn btn-secondary cart-btn-transform\">continue\n                        shopping</router-link>\n                </td>\n                <td>\n                    <router-link :to=\"'/ecommerce/checkout'\" class=\"btn btn-success cart-btn-transform\"> check\n                        out</router-link>\n                </td>\n            </tr>\n        </tbody>\n    </table>\n</template>\n<script>\nimport { mapState } from \"vuex\";\nimport getImage from \"@/mixins/getImage\"\nexport default {\n    mixins: [getImage],\n    data() {\n        return {\n            counter: 1,\n        };\n    },\n    computed: {\n        ...mapState({\n            cart: (state) => state.product.cart,\n            getAmount() { return (this.totalAmount = this.$store.getters[\"product/getTotalAmount\"]); },\n        }),\n    },\n    methods: {\n        removeProduct: function (product) { this.$store.dispatch(\"product/removeProduct\", product); },\n        increment(product, qty = 1) {\n            this.$store.dispatch(\"product/updateCartQuantity\", {\n                product: product,\n                qty: qty,\n            });\n        },\n        decrement(product, qty = -1) {\n            this.$store.dispatch(\"product/updateCartQuantity\", {\n                product: product,\n                qty: qty,\n            });\n        },\n    },\n};\n</script>"],"mappings":"AAoEA,SAASA,QAAO,QAAS,MAAM;AAC/B,OAAOC,QAAO,MAAO,mBAAkB;AACvC,eAAe;EACXC,MAAM,EAAE,CAACD,QAAQ,CAAC;EAClBE,IAAIA,CAAA,EAAG;IACH,OAAO;MACHC,OAAO,EAAE;IACb,CAAC;EACL,CAAC;EACDC,QAAQ,EAAE;IACN,GAAGL,QAAQ,CAAC;MACRM,IAAI,EAAGC,KAAK,IAAKA,KAAK,CAACC,OAAO,CAACF,IAAI;MACnCG,SAASA,CAAA,EAAG;QAAE,OAAQ,IAAI,CAACC,WAAU,GAAI,IAAI,CAACC,MAAM,CAACC,OAAO,CAAC,wBAAwB,CAAC;MAAG;IAC7F,CAAC;EACL,CAAC;EACDC,OAAO,EAAE;IACLC,aAAa,EAAE,SAAAA,CAAUN,OAAO,EAAE;MAAE,IAAI,CAACG,MAAM,CAACI,QAAQ,CAAC,uBAAuB,EAAEP,OAAO,CAAC;IAAE,CAAC;IAC7FQ,SAASA,CAACR,OAAO,EAAES,GAAE,GAAI,CAAC,EAAE;MACxB,IAAI,CAACN,MAAM,CAACI,QAAQ,CAAC,4BAA4B,EAAE;QAC/CP,OAAO,EAAEA,OAAO;QAChBS,GAAG,EAAEA;MACT,
{"ast":null,"code":"import { ref } from \"vue\";\nimport { Swiper, SwiperSlide } from \"swiper/vue\";\nimport productPage from \"@/mixins/productPage\";\nimport getImage from \"@/mixins/getImage\";\nimport \"swiper/css\";\nimport { Autoplay, FreeMode, Navigation, Thumbs } from \"swiper\";\nexport default {\n  mixins: [productPage, getImage],\n  name: \"productPage\",\n  components: {\n    Swiper,\n    SwiperSlide\n  },\n  setup() {\n    const thumbsSwiper = ref(null);\n    const setThumbsSwiper = swiper => {\n      thumbsSwiper.value = swiper;\n    };\n    return {\n      modules: [Autoplay, Navigation, FreeMode, Thumbs],\n      thumbsSwiper,\n      setThumbsSwiper\n    };\n  },\n  methods: {\n    filled(len) {\n      this.selectedStars = len;\n    },\n    addToCart: function (product, qty) {\n      product.quantity = qty ? qty : 1;\n      this.$store.dispatch(\"product/addToCart\", product);\n    }\n  }\n};","map":{"version":3,"names":["ref","Swiper","SwiperSlide","productPage","getImage","Autoplay","FreeMode","Navigation","Thumbs","mixins","name","components","setup","thumbsSwiper","setThumbsSwiper","swiper","value","modules","methods","filled","len","selectedStars","addToCart","product","qty","quantity","$store","dispatch"],"sources":["D:\\Wrenix\\Spring-Sfa\\Cuba\\template\\template\\src\\pages\\ecommerce\\productpage\\ProductSwiper.vue"],"sourcesContent":["<template>\n    <div class=\"col-xxl-4 col-md-6 box-col-12\">\n        <div class=\"card\">\n            <div class=\"card-body\">\n                <Swiper :slidesPerView=\"1\" :loop=\"true\" :thumbs=\"{ swiper: thumbsSwiper }\"\n                    :autoplay=\"{ delay: 3500, disableOnInteraction: false }\" :spaceBetween=\"30\" :centeredSlides=\"true\"\n                    :modules=\"modules\" dir=\"rtl\" class=\"mySwiper\">\n                    <swiper-slide v-for=\"(product, index) in products.images\" :key=\"index\">\n                        <img :src=\"getImageUrl(product)\" :id=\"index\" class=\"img-fluid bg-img\" alt=\"index\" />\n                    </swiper-slide>\n                </Swiper>\n                <Swiper @swiper=\"setThumbsSwiper\" :loop=\"true\" :slidesPerView=\"3\" :spaceBetween=\"3\"\n                    :watchSlidesProgress=\"true\" :pagination=\"{\n                        clickable: true,\n                    }\" :modules=\"modules\" dir=\"rtl\" class=\"Swiper mt-2\">\n\n                    <swiper-slide v-for=\"(product, index) in products.images\" :key=\"index\">\n                        <img :src=\"getImageUrl(product)\" :id=\"index\" class=\"img-fluid bg-img\" alt=\"index\"\n                            style=\"height:auto; \" />\n                    </swiper-slide>\n                </Swiper>\n            </div>\n        </div>\n</div>\n</template>\n<script>\nimport { ref } from \"vue\";\nimport { Swiper, SwiperSlide } from \"swiper/vue\";\nimport productPage from \"@/mixins/productPage\"\nimport getImage from \"@/mixins/getImage\"\nimport \"swiper/css\";\nimport { Autoplay, FreeMode, Navigation, Thumbs } from \"swiper\";\n\nexport default {\n    mixins: [productPage, getImage],\n    name: \"productPage\",\n    components: {\n        Swiper,\n        SwiperSlide,\n    },\n    setup() {\n        const thumbsSwiper = ref(null);\n        const setThumbsSwiper = (swiper) => {\n            thumbsSwiper.value = swiper;\n        };\n        return {\n            modules: [Autoplay, Navigation, FreeMode, Thumbs],\n            thumbsSwiper,\n            setThumbsSwiper,\n        };\n    },\n    methods: {\n        filled(len) {\n            this.selectedStars = len;\n        },\n        addToCart: function (product, qty) {\n            product.quantity = qty ? qty : 1;\n            this.$store.dispatch(\"product/addToCart\", product);\n        },\n    },\n};\n</script>"],"mappings":"AA0BA,SAASA,GAAE,QAAS,KAAK;AACzB,SAASC,MAAM,EAAEC,WAAU,QAAS,YAAY;AAChD,OAAOC,WAAU,MAAO,sBAAqB;AAC7C,OAAOC,QAAO,MAAO,mBAAkB;AACvC,OAAO,YAAY;AACnB,SAASC,QAAQ,EAAEC,QAAQ,EAAEC
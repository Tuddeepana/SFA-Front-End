{"ast":null,"code":"import { mapState } from 'vuex';\nimport getImage from \"@/mixins/getImage\";\nexport default {\n  mixins: [getImage],\n  computed: {\n    ...mapState({\n      card: state => state.common.usercard\n    })\n  }\n};","map":{"version":3,"names":["mapState","getImage","mixins","computed","card","state","common","usercard"],"sources":["D:\\Wrenix\\Spring-Sfa\\Cuba\\template\\template\\src\\pages\\users\\cards\\card1.vue"],"sourcesContent":["<template>\n    <div class=\"col-xl-4 col-sm-6 col-xxl-3 col-ed-4 box-col-4\" v-for=\"(item, index) in card\" :key=\"index\">\n        <div class=\"card social-profile\">\n            <div class=\"card-body\">\n                <div class=\"social-img-wrap\">\n                    <div class=\"social-img\"><img :src=\"getImageUrl(item.avatar)\" alt=\"profile\"></div>\n                    <div class=\"edit-icon\">\n                        <svg>\n                            <use href=\"@/assets/svg/icon-sprite.svg#profile-check\"></use>\n                        </svg>\n                    </div>\n                </div>\n                <div class=\"social-details\">\n                    <h5 class=\"mb-1\"><router-link to=\"/app/socialPage\">{{ item.name }}</router-link></h5><span\n                        class=\"f-light\">{{ item.email\n                        }}</span>\n                    <ul class=\"card-social\">\n                        <li><a href=\"https://www.facebook.com/\" target=\"_blank\"><i class=\"fa fa-facebook\"></i></a></li>\n                        <li><a href=\"https://accounts.google.com/\" target=\"_blank\"><i class=\"fa fa-google-plus\"></i></a>\n                        </li>\n                        <li><a href=\"https://twitter.com/\" target=\"_blank\"><i class=\"fa fa-twitter\"></i></a></li>\n                        <li><a href=\"https://www.instagram.com/\" target=\"_blank\"><i class=\"fa fa-instagram\"></i></a></li>\n                        <li><a href=\"https://rss.app/\" target=\"_blank\"><i class=\"fa fa-rss\"></i></a></li>\n                    </ul>\n                    <ul class=\"social-follow\">\n                        <li>\n                            <h5 class=\"mb-0\">{{ item.totalPost }}</h5><span class=\"f-light\">Posts</span>\n                        </li>\n                        <li>\n                            <h5 class=\"mb-0\">{{ item.follower }}</h5><span class=\"f-light\">Followers</span>\n                        </li>\n                        <li>\n                            <h5 class=\"mb-0\">{{ item.following }}</h5><span class=\"f-light\">Following</span>\n                        </li>\n                    </ul>\n                </div>\n            </div>\n        </div>\n    </div>\n</template>\n<script>\nimport { mapState } from 'vuex';\nimport getImage from \"@/mixins/getImage\"\nexport default {\n    mixins: [getImage],\n    computed: {\n        ...mapState({\n            card: (state) => state.common.usercard,\n\n        }),\n    },\n}\n</script>"],"mappings":"AAyCA,SAASA,QAAO,QAAS,MAAM;AAC/B,OAAOC,QAAO,MAAO,mBAAkB;AACvC,eAAe;EACXC,MAAM,EAAE,CAACD,QAAQ,CAAC;EAClBE,QAAQ,EAAE;IACN,GAAGH,QAAQ,CAAC;MACRI,IAAI,EAAGC,KAAK,IAAKA,KAAK,CAACC,MAAM,CAACC;IAElC,CAAC;EACL;AACJ","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}
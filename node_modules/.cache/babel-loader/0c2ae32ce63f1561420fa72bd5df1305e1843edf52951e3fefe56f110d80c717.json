         <div class=\"input-group\">\n                    <select v-model=\"employee.gender\" class=\"form-select\">\n                      <option value=\"MALE\">Male</option>\n                      <option value=\"FEMALE\">Female</option>\n                      <option value=\"OTHER\">Other</option>\n                    </select>\n                  </div>\n                </div>\n\n                <div class=\"col-md-6 mb-3\">\n                  <label class=\"form-label\">Status</label>\n                  <div class=\"input-group\">\n                    <select v-model=\"employee.status\" class=\"form-select\">\n                      <option value=\"ACTIVE\">Active</option>\n                      <option value=\"INACTIVE\">Inactive</option>\n                      <option value=\"PENDING\">Pending</option>\n                    </select>\n                  </div>\n                </div>\n              </div>\n            </form>\n          </div>\n        </div>\n      </div>\n      <div class=\"card-footer\">\n        <button class=\"btn btn-primary m-r-15\" type=\"submit\" @click.prevent=\"submitForm\" :disabled=\"loading\">\n          <span v-if=\"loading\" class=\"spinner-border spinner-border-sm mr-2\"></span>\n          {{ loading ? 'Saving...' : 'Submit' }}\n        </button>\n        <button class=\"btn btn-light\" type=\"button\" :disabled=\"loading\">Cancel</button>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\nimport apiService from '../../../services/apiService';\n\nexport default {\n  data() {\n    return {\n      employee: {\n        firstName: \"\",\n        lastName: \"\",\n        email: \"\",\n        dob: \"\",\n        address: \"\",\n        status: \"ACTIVE\",\n        gender: \"MALE\"\n      },\n      loading: false,\n      errorMessage: \"\",\n      successMessage: \"\"\n    };\n  },\n  methods: {\n    async submitForm() {\n      this.loading = true;\n      this.errorMessage = \"\";\n      this.successMessage = \"\";\n\n     
 colors: ['#fff'],\n                    strokeColor: primary,\n                    strokeWidth: 2,\n                },\n                tooltip: {\n                    y: {\n                        formatter: function (val) {\n                            return val\n                        }\n                    }\n                },\n                yaxis: {\n                    tickAmount: 7,\n                    labels: {\n                        formatter: function (val, i) {\n                            if (i % 2 === 0) {\n                                return val\n                            } else {\n                                return ''\n                            }\n                        }\n                    }\n                },\n\n            }\n        }\n    }\n}\n</script>\n"],"mappings":"AAgBA,IAAIA,OAAM,GAAIC,YAAY,CAACC,OAAO,CAAC,eAAe,KAAK,SAAS;AAChE,IAAIC,SAAQ,GAAIF,YAAY,CAACC,OAAO,CAAC,iBAAiB,KAAK,SAAS;AACpE,eAAe;EACXE,IAAIA,CAAA,EAAG;IACH,OAAO;MACHC,MAAM,EAAE,CAAC;QACLC,IAAI,EAAE,UAAU;QAChBF,IAAI,EAAE,CAAC,EAAE,EAAE,GAAG,EAAE,EAAE,EAAE,EAAE,EAAE,EAAE,EAAE,EAAE,EAAE,EAAE;MACtC,CAAC,CAAC;MACFG,YAAY,EAAE;QACVC,KAAK,EAAE;UACHC,MAAM,EAAE,GAAG;UACXC,IAAI,EAAE,OAAO;UACbC,OAAO,EAAE;YACLC,IAAI,EAAE;UACV;QACJ,CAAC;QACDP,MAAM,EAAE,CAAC;UACLC,IAAI,EAAE,UAAU;UAChBF,IAAI,EAAE,CAAC,EAAE,EAAE,GAAG,EAAE,EAAE,EAAE,EAAE,EAAE,EAAE,EAAE,EAAE,EAAE,EAAE;QACtC,CAAC,CAAC;QACFS,MAAM,EAAE,CAAC,QAAQ,EAAE,QAAQ,EAAE,SAAS,EAAE,WAAW,EAAE,UAAU,EAAE,QAAQ,EAAE,UAAU,CAAC;QACtFC,WAAW,EAAE;UACTC,KAAK,EAAE;YACHC,IAAI,EAAE,GAAG;YACTC,QAAQ,EAAE;cACNC,WAAW,EAAElB,OAAO;cACpBmB,IAAI,EAAE;gBACFC,MAAM,EAAE,CAAC,SAAS,EAAE,MAAM;cAC9B;YACJ;UACJ;QACJ,CAAC;QACDC,KAAK,EAAE;UACHC,IAAI,EAAE;QACV,CAAC;QACD;QACAF,MAAM,EAAE,CAACjB,SAAS,CAAC;QACnBoB,OAAO,EAAE;UACLP,IAAI,EAAE,CAAC;UACPI,MAAM,EAAE,CAAC,MAAM,CAAC;UAChBF,WAAW,EAAElB,OAAO;UACpBwB,WAAW,EAAE;QACjB,CAAC;QACDC,OAAO,EAAE;UACLC,CAAC,EAAE;YACCC,SAAS,EAAE,SAAAA,CAAUC,GAAG,EAAE;cACtB,OAAOA,GAAE;YACb;UACJ;QACJ,CAAC;QACDC,KAAK,EAAE;UACHC,UAAU,